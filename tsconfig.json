{
	"extends": "@tsconfig/strictest/tsconfig.json",
	"include": ["src-js"],
	"compilerOptions": {
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": false,
		"exactOptionalPropertyTypes": false,
		"verbatimModuleSyntax": true,
		"noEmit": true,
		"moduleResolution": "Node",
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM"],
		"types": []
	}
}
